
{
	"name": "seppzzz/html-editor-scroll",
    "description": "A Tinymce plugin for HtmlEditorField to scroll to last position before saved.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "seppzzz",
        "HtmlEditorfield",
        "Tinymce plugin",
		"scroll to position"
    ],
    "authors": [
        {
            "name": "seppzzz",
            "email": "seppzzz@till.graphics"
        }
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "^4.0 || ^5.0",  
        "silverstripe/login-forms": "^4.9 || ^5.0", 
        "php": "^7.2 || ^8.0" 
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"  
    },
    "autoload": {
        "psr-4": {
            "Seppzzz\\Scroll\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "stable", 
    "prefer-stable": true
}
